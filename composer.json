{
	"name": "dynamic/core-tools",
	"description": "core tools to build common page types",
	"authors": [
		{
			"name": "Dynamic",
			"email": "dev@dynamicagency.com"
		}
	],
	"keywords": [
		"silverstripe"
	],
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"repositories": [
		{
			"type": "composer",
			"url": "https://repo.packagist.com/dynamic/"
		}
	],
	"require": {
		"silverstripe/recipe-cms": "^1@dev || ^4@dev",
		"silverstripe/vendor-plugin": "^1@dev",
		"dynamic/silverstripe-geocoder": "^1@dev",
		"silvershop/silverstripe-hasonefield": "^2.1@dev",
		"symbiote/silverstripe-gridfieldextensions": "^3@dev",
		"unclecheese/display-logic": "^2@dev",
		"dnadesign/silverstripe-elemental": "^2@dev || ^3@dev || ^4@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"squizlabs/php_codesniffer": "^3.0",
		"phpmd/phpmd": "^2.6",
		"sebastian/phpcpd": "^3.0",
		"phploc/phploc": "^4.0",
		"pdepend/pdepend": "^2.5",
		"theseer/phpdox": "^0.11.0"
	},
	"config": {
		"process-timeout": 600
	},
	"autoload": {
		"psr-4": {
			"Dynamic\\CoreTools\\": "src/",
			"Dynamic\\CoreTools\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"installer-name": "core-tools",
		"branch-alias": {
			"dev-master": "4.0.x-dev"
		}
	}
}
