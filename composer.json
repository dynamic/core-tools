{
  "name": "dynamic/core-tools",
  "description": "core tools to build common page types",
  "authors": [
    {
      "name": "Dynamic",
      "email": "dev@dynamicagency.com"
    }
  ],
  "keywords": [
    "silverstripe"
  ],
  "type": "silverstripe-module",
  "license": "BSD-3-Clause",
  "repositories": [
    {
      "type": "composer",
      "url": "https://dynamicinc:I68blegDd@toran.dynamicagency.com/repo/private/"
    }
  ],
  "require": {
    "silverstripe/cms": "^4.0",
    "silverstripe/framework": "^4.0",
    "silverstripe/asset-admin": "^1.0",
    "silverstripe-australia/gridfieldextensions": "2.0.x-dev"
  },
  "config": {
    "process-timeout": 600
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\CoreTools\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-name": "core-tools",
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
